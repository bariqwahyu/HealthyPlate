<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="txtDescription" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/league_spartan_regular</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="txtDescription.ET">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="tvET" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:alpha">0</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/black</item>
    </style>
</resources>